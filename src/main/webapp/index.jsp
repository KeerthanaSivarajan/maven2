    .item img:hover{
        transform: scale(1.04);
    }
    .item h4{
        padding: 5px;
        text-align: center;
        font-size: 20px;
    
    }
    .item button{
        padding: 5px 30px;
        border:none;
        outline:none;
        background-color: green;
        color:white;
        cursor: pointer;
        border-radius: 4px;
        font-size: 20px;
        display: block;
        margin: auto;
    }
    .item button:hover{
        transform: scale(1.04);
    }
